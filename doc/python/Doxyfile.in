DOXYFILE_ENCODING      = UTF-8
PROJECT_NAME           = "CreDB"

INPUT = "@TOP_BUILDDIR@/doc/python"

# THEME
GENERATE_HTML=YES
HTML_EXTRA_STYLESHEET="@TOP_SRCDIR@/doc/theme/style.css"

OUTPUT_DIRECTORY="python-doc"

# Label is only used for our custom scripts
ALIASES += label{1}=""

ENABLE_PREPROCESSING = YES
